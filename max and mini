#include <iostream>
using namespace std;

int main() {
    int num;
    cin >> num;

    int arr[num];
    for (int i = 0; i < num; i++) {
        cin >> arr[i];
    }

    int maxi = arr[0];
    int mini = arr[0];

    for (int i = 1; i < num; i++) {
        if (arr[i] > maxi) {
            maxi = arr[i];
        }
        if (arr[i] < mini) {
            mini = arr[i];
        }
    }

    cout << "Max: " << maxi << "  Min: " << mini << endl;
    return 0;
}



























